<configuration>
	<appender name="FILE" class="ch.qos.logback.core.FileAppender">
		<file>Analyst.log</file>
		<append>true</append>
		<encoder>
			<pattern>%date{HH:mm:ss.SSS} [%16thread]  %-5level  %logger{20} : %message%n</pattern>
		</encoder>
	</appender>
	<root level="info">
		<appender-ref ref="FILE" />
	</root>
</configuration>
